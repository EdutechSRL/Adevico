<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="ErrorsNotificationService" connectionString="Server=localhost;Initial Catalog=ErrorsService;User ID=legge6;Password=lmadm1" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="ErrorsQueueName" value=".\private$\farm_errors" />
    <add key="PoisonErrorsQueueName" value=".\private$\farm_posionerrors" />
    <add key="DBconnection" value="ErrorsNotificationService" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="MetadataBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceCredentials>
            <clientCertificate>
              <authentication certificateValidationMode="None" revocationMode="NoCheck" includeWindowsGroups="false" />
