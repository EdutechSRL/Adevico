        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9779/ErrorsNotification/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netMsmqBinding>
        <binding name="ErrorsNotificationNetMsmqBinding" deadLetterQueue="Custom">
          <security mode="None">
            <transport msmqAuthenticationMode="None" msmqProtectionLevel="None" />
